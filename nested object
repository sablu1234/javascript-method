## nested object
let profile ={
    name: "tapas",
    company: "CreoWis",
    message: function(){
        console.log(`${this.name} work at ${this.company}`);
    },
    address: {
        city: "Bangalore",
        pin: 56032,
        state: "Karnataka",
        country: "India",
        greeting: function(){
            console.log("Welcome to India");
        }
    },
}
console.log(profile.name);//tapas
console.log(profile.company);//CreoWis
profile.message();
