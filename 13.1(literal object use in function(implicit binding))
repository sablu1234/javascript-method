// 13.1(literal object use in function)
const tom = {
    'name': 'Tom',
    'age': 7
}

const jerry ={
    'name': 'jerry',
    'age': 3
}

function greetMe(obj){
    obj.logMessage = function(){
        console.log(`${this.name} is ${this.age} years old`);
    }
    console.log(obj);
}

greetMe(tom);
tom.logMessage();
