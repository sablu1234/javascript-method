// Inside an object(amara jokhon kono method je object ar modhe call korbo sei method ar "this" ar value sei object ar value hobe)

const employee = {
    id: 65465,
    firstName: "Alex",
    lastName: "B",
    returnThis: function(){
        return this;
    }
}

console.log("Employedd ID: ",employee.id);
console.log(employee.returnThis())
