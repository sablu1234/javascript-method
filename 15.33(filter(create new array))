#Now see the usecase #amra critacal array theke filter kore new array create 
// filter()
let customers =[
  {
    'id': 001,
    'f_name': 'Abby',
    'l_name': 'Thomas',
    'gender': 'M',
    'married': true,
    'age': 32,
