//"use stict" use korle undefinde hoi(kono function baire theke access korte hole return korte hoi)
// inside funciton 
function sayHello(){
    console.log("this inside a fucniton", this)
}

sayHello();

function outer(a){
    console.log("this inside an outer function");

    return function inner(b){
        console.log("this inside an inner fucntion");
    }
}

const outerResult = outer(5);
outerResult(3);
